{
	"type": "GStreamer",
	"template": ["urisourcebin name=source ! concat name=c ! decodebin ! video/x-raw ! videoconvert name=videoconvert",
				" ! gvadetect model={models[object_detection][1][network]} name=detection",
				" ! gvametaconvert name=metaconvert ! queue ! gvametapublish name=destination",
				" ! appsink name=appsink"
				],
	"description": "Object Detection Pipeline",
	"parameters": {
		"type": "object",
		"properties": {
			"device": {
				"element": "detection",
				"type": "string"
			},
			"detection-model-instance-id": {
				"element": {
					"name": "detection",
					"property": "model-instance-id"
				},
				"type": "string",
				"default": "inf0"
			},
			"inference-interval": {
				"element": "detection",
				"type": "integer",
				"minimum": 1,
				"maximum": 4294967295,
				"default": 1
			},
			"cpu-throughput-streams": {
				"element": "detection",
				"type": "integer",
				"minimum": 0,
				"maximum": 4294967295
			},
			"n-threads": {
				"element": "videoconvert",
				"type": "integer",
				"default": 1
			},
			"nireq": {
				"element": "detection",
				"type": "integer",
				"minimum": 1,
				"maximum": 64,
				"default": 2
			}
		}
	}
}
