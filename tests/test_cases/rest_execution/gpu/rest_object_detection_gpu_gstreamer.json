{
    "path": "pipelines/object_detection/person_vehicle_bike",
    "start" : {
        "body": {
            "source": {
                "uri": "file:///home/video-analytics-serving/tests/media/person-bicycle-car-detection.mp4",
                "type": "uri"
            },
            "destination": {
                "type": "file",
                "path": "/tmp/rest_object_detection",
                "format": "json-lines"
            },
            "parameters": {
                "detection-device": "GPU"
            }
        }
    },
    "check_running" : {
        "timeout": 60.0
    },
    "abort" : {
        "delay" : 0.5
    },
    "check_stopped" : {
        "timeout": 0.5
    }
}
