{
    "path": "pipelines/object_detection/person_vehicle_bike",
    "start": {
        "body": {
            "source": {
                "uri": "https://lvamedia.blob.core.windows.net/public/homes_00425.mkv",
                "type": "uri"
            },
            "destination": {
                "metadata": {
                    "type": "file",
                    "path": "/tmp/rest_object_detection",
                    "format": "json-lines"
                },
                "frame": {
                    "type": "rtsp",
                    "path": "person"
                }
            },
            "parameters": {
                "detection-device": "CPU"
            }
        }
    },
    "check_running": {
        "timeout": 2.0
    },
    "check_rtsp_stream": {
        "delay": 0.2,
        "url": "rtsp://localhost",
        "port": 8554
    },
    "abort": {
        "delay": 0.5
    },
    "check_stopped": {
        "timeout": 0.5
    },
    "result": 2
}